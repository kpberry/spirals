                         <Color blue="0.04" green="0.02" red="0.01" />
                     </value>
                 </ColorPicker>
                 <ColorPicker fx:id="secondaryColorPicker" prefHeight="25.0" prefWidth="246.0" GridPane.columnIndex="1" GridPane.rowIndex="5">
                     <value>
                         <Color green="1.0" />
